import "variadics"

fun printf(message: u8*, args: ...): i32

fun vprintf(message: u8*, args: va_list): i32 

fun println(message: u8*, args: ...): i32 {
    const list = va_start()
    
    const length = vprintf(message, list)
    printf("\n")
    
    va_end(list);
    return length
}
