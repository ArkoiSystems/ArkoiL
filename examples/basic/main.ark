import "console"

fun main(): i32 {
    printf("%i\n", fibonacci(35))
    printf("%i\n", pow(3, 3))
    return 0
}

fun pow(x, y: i32): i32 {
    if(y < 0)
        return -1
    if(y == 0)
        return 1
    
    return pow(x, y - 1) * x
}

fun fibonacci(x: i32): i32 {
    if(x == 0) 
        return 0
    else if(x == 1)
        return 1
    
    return fibonacci(x - 1) + fibonacci(x - 2)
}
